        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Dictionary1.xaml" />
                <ResourceDictionary Source="Dictionary2.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <local:DataConverter x:Key="DataConverter"/>
            <Style TargetType="CheckBox" x:Key="CheckBoxClickStyle">
                <EventSetter Event="Click" Handler="CheckBoxClick"/>
            </Style>
            <Style TargetType="Button" x:Key="ButtonMouseEventStyle">
                <EventSetter Event="MouseEnter" Handler="ButtonMouseEnter"/>
                <EventSetter Event="MouseLeave" Handler="ButtonMouseLeave"/>
                <EventSetter Event="PreviewMouseDown" Handler="ButtonMouseDown"/>
                <EventSetter Event="PreviewMouseUp" Handler="ButtonMouseUp"/>
            </Style>
            <l:StyleCombiner TargetType="Button" x:Key="ButtonRepaintColStyle" x:Shared="False">
                <l:ParameterizedResource ResourceKey="ButtonRepaintColStyleBase">
                    <x:Array Type="sys:String">
                        <sys:String>$currentMouseEnter:CurrentMouseEnter</sys:String>
                        <sys:String>$dataConverter:DataConverter</sys:String>
                        <sys:String>$isMouseEnter:IsMouseEnter</sys:String>
                    </x:Array>
                </l:ParameterizedResource>
            </l:StyleCombiner>
            <l:StyleCombiner TargetType="Button" x:Key="ButtonRepaintRowStyle" x:Shared="False">
                <l:ParameterizedResource ResourceKey="ButtonRepaintRowStyleBase">
                    <x:Array Type="sys:String">
                        <sys:String>$currentMouseEnter:CurrentMouseEnter</sys:String>
                        <sys:String>$dataConverter:DataConverter</sys:String>
                        <sys:String>$isMouseEnter:IsMouseEnter</sys:String>
                    </x:Array>
                </l:ParameterizedResource>
            </l:StyleCombiner>
            <Style TargetType="Button" x:Key="ButtonAnimationStyle" x:Shared="False">
                <Style.Triggers>
                    <DataTrigger Binding="{Binding CurrentMouseDown, Converter={l:ParameterizedResource DataConverter}, ConverterParameter=$coordinates|IsMouseDown}" Value="True">
                        <DataTrigger.EnterActions>
                            <BeginStoryboard>
                                <Storyboard>
                                    <DoubleAnimation Storyboard.TargetProperty="Opacity" From="1" To="0" Duration="0:0:0.5" AutoReverse="True" RepeatBehavior="1x" />
                                </Storyboard>
                            </BeginStoryboard>
                        </DataTrigger.EnterActions>
                    </DataTrigger>
                </Style.Triggers>
            </Style>
            <Style TargetType="Button" x:Key="ButtonContentStyle" x:Shared="False">
                <Setter Property="Content" Value="{Binding RelativeSource={RelativeSource Self}, Converter={l:ParameterizedResource DataConverter}, ConverterParameter=Content}"/>
                <Setter Property="FontSize" Value="22.0"/>
            </Style>
            <l:BindingProxy x:Key="IsEditable" Value="{Binding IsDatasEditable}"/>
            <l:BindingProxy x:Key="NotEditable" Value="{Binding Source=False}"/>
            <l:BindingProxy x:Key="RemoveCommand" Value="{Binding RemoveCommand}"/>
            <l:BindingProxy x:Key="CurrentEditedItem"/>
        </ResourceDictionary>
    </Window.Resources>
