        <TabItem Header="Demo2">
            <DockPanel LastChildFill="True">
                <StackPanel Orientation="Horizontal" DockPanel.Dock="Top" Margin="10,10,10,10">
                    <TextBlock>IsEditable</TextBlock>
                    <CheckBox IsChecked="{Binding IsDatasEditable, Mode=OneWayToSource}"></CheckBox>
                    <Button Content="➕" Command="{Binding AddCommand}" CommandParameter="{Binding IsDatasEditable}" Margin="10,0,0,0" ToolTip="Add row"/>
                </StackPanel>
                <TextBlock DockPanel.Dock="Bottom" Text="{Binding Datas.Count}" Margin="10,10,10,10"/>
                <DataGrid ItemsSource="{Binding DatasViewSource.View}" AutoGenerateColumns="False" Margin="10,10,10,10" 
                          CanUserAddRows="False" x:Name="DataGrid">
                    <DataGrid.Resources>
                        <sys:String x:Key="Item1">Item1</sys:String>
                        <sys:String x:Key="Item2">Item2</sys:String>
                        <sys:String x:Key="Item3">Item3</sys:String>
                        <sys:String x:Key="Item4">Item4</sys:String>
                        <sys:String x:Key="Item5">Item5</sys:String>
                        <l:ParameterizedResource x:Key="CellDataTemplate" x:Shared="False" ResourceKey="DataTemplate">
                            <x:Array Type="sys:String">
                                <sys:String>$dataConverter:DataConverter</sys:String>
                                <sys:String>$isEditable:NotEditable</sys:String>
                            </x:Array>
                        </l:ParameterizedResource>
                        <l:ParameterizedResource x:Key="CellEditingDataTemplate" x:Shared="False" ResourceKey="DataTemplate">
                            <x:Array Type="sys:String">
                                <sys:String>$dataConverter:DataConverter</sys:String>
                                <sys:String>$isEditable:IsEditable</sys:String>
                            </x:Array>
                        </l:ParameterizedResource>
                        <DataTemplate x:Key="IsReadOnly">
                            <CheckBox IsChecked="{Binding IsReadOnly, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                        </DataTemplate>
                        <DataTemplate x:Key="Actions">
                            <StackPanel Orientation="Horizontal">
                                <Button Content="➖" Command="{Binding Value, Source={StaticResource RemoveCommand}}" ToolTip="Remove row">
                                    <Button.CommandParameter>
                                        <MultiBinding Converter="{l:ParameterizedResource DataConverter}" ConverterParameter="RemoveCommandCanExecute">
                                            <Binding Path="."/>
                                            <Binding Path="Value" Source="{StaticResource IsEditable}"/>
                                        </MultiBinding>
                                    </Button.CommandParameter>
                                </Button>
                            </StackPanel>
                        </DataTemplate>
                    </DataGrid.Resources>
                    <DataGrid.Columns>
                        <DataGridTemplateColumn Header="IsReadOnly" CellEditingTemplate="{StaticResource IsReadOnly}">
                            <DataGridTemplateColumn.CellTemplate>
                                <DataTemplate>
                                    <TextBlock Text="{Binding IsReadOnly}"/>
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
                        </DataGridTemplateColumn>
                        <DataGridTemplateColumn Header="Item1"
                                                CellTemplateSelector="{l:ParameterizedResource CellDataTemplate, Replaces=$fieldName:Item1}"
                                                CellEditingTemplateSelector="{l:ParameterizedResource CellEditingDataTemplate, Replaces=$fieldName:Item1}"/>
                        <DataGridTemplateColumn Header="Item2" 
                                                CellTemplateSelector="{l:ParameterizedResource CellDataTemplate, Replaces=$fieldName:Item2}"
                                                CellEditingTemplateSelector="{l:ParameterizedResource CellEditingDataTemplate, Replaces=$fieldName:Item2}"/>
                        <DataGridTemplateColumn Header="Item3" 
                                                CellTemplateSelector="{l:ParameterizedResource CellDataTemplate, Replaces=$fieldName:Item3}"
                                                CellEditingTemplateSelector="{l:ParameterizedResource CellEditingDataTemplate, Replaces=$fieldName:Item3}"/>
                        <DataGridTemplateColumn Header="Item4" 
                                                CellTemplateSelector="{l:ParameterizedResource CellDataTemplate, Replaces=$fieldName:Item4}"
                                                CellEditingTemplateSelector="{l:ParameterizedResource CellEditingDataTemplate, Replaces=$fieldName:Item4}"/>
                        <DataGridTemplateColumn Header="Item5" 
                                                CellTemplateSelector="{l:ParameterizedResource CellDataTemplate, Replaces=$fieldName:Item5}"
                                                CellEditingTemplateSelector="{l:ParameterizedResource CellEditingDataTemplate, Replaces=$fieldName:Item5}"/>
                        <DataGridTemplateColumn Header="Actions" CellTemplate="{StaticResource Actions}"/>
                    </DataGrid.Columns>
                </DataGrid>
            </DockPanel>
        </TabItem>
        <TabItem Header="Demo3">
            <DockPanel LastChildFill="True">
