        <TabItem Header="Demo2">
            <DockPanel LastChildFill="True">
                <StackPanel Orientation="Horizontal" DockPanel.Dock="Top" Margin="10,10,10,10">
                    <TextBlock>IsEditable</TextBlock>
                    <CheckBox IsChecked="{Binding IsDatasEditable, Mode=OneWayToSource}"></CheckBox>
                    <Button Content="➕" Command="{Binding AddCommand}" CommandParameter="{Binding IsDatasEditable}" Margin="10,0,0,0" ToolTip="Add row"/>
                </StackPanel>
                <TextBlock DockPanel.Dock="Bottom" Text="{Binding Datas.Count}" Margin="10,10,10,10"/>
                <DataGrid ItemsSource="{Binding DatasViewSource.View}" AutoGenerateColumns="False" Margin="10,10,10,10" 
                          CanUserAddRows="False" x:Name="DataGrid">
                    <DataGrid.Resources>
                        <sys:String x:Key="Item1">Item1</sys:String>
                        <sys:String x:Key="Item2">Item2</sys:String>
                        <sys:String x:Key="Item3">Item3</sys:String>
                        <sys:String x:Key="Item4">Item4</sys:String>
                        <sys:String x:Key="Item5">Item5</sys:String>
                        
                        <DataTemplate x:Key="EditField" x:Shared="False">
                            <DockPanel LastChildFill="true">
                                <ComboBox DockPanel.Dock="Left" SelectedItem="{Binding $fieldName.Type, Converter={local:DataConverter}, ConverterParameter=FieldTypeText, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
                                    <ComboBox.ItemsSource>
                                        <x:Array Type="sys:String">
                                            <sys:String>String</sys:String>
                                            <sys:String>Int32</sys:String>
                                            <sys:String>Double</sys:String>
                                            <sys:String>Boolean</sys:String>
                                            <sys:String>DateOnly</sys:String>
                                            <sys:String>Activities</sys:String>
                                        </x:Array>
                                    </ComboBox.ItemsSource>
                                </ComboBox>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition>
                                            <RowDefinition.Style>
                                                <Style TargetType="RowDefinition">
                                                    <Style.Triggers>
                                                        <DataTrigger Binding="{Binding $fieldName.Type, Converter={local:DataConverter}, ConverterParameter=FieldTypeText}"
                                                                    Value="DateOnly">
                                                            <Setter Property="Height" Value="0"/>
                                                        </DataTrigger>
                                                        <DataTrigger Binding="{Binding $fieldName.Type, Converter={local:DataConverter}, ConverterParameter=FieldTypeText}"
                                                                    Value="Boolean">
                                                            <Setter Property="Height" Value="0"/>
                                                        </DataTrigger>
                                                        <DataTrigger Binding="{Binding $fieldName.Type, Converter={local:DataConverter}, ConverterParameter=FieldTypeText}"
                                                                    Value="Activities">
                                                            <Setter Property="Height" Value="0"/>
                                                        </DataTrigger>
                                                    </Style.Triggers>

                                                </Style>
                                            </RowDefinition.Style>
                                        </RowDefinition>
                                        <RowDefinition>
                                            <RowDefinition.Style>
                                                <Style TargetType="RowDefinition">
                                                    <Style.Triggers>
                                                        <DataTrigger Binding="{Binding $fieldName.Type, Converter={local:DataConverter}, ConverterParameter=FieldTypeText}"
                                                                     Value="String">
                                                            <Setter Property="Height" Value="0"/>
                                                        </DataTrigger>
                                                        <DataTrigger Binding="{Binding $fieldName.Type, Converter={local:DataConverter}, ConverterParameter=FieldTypeText}"
                                                                     Value="Int32">
                                                            <Setter Property="Height" Value="0"/>
                                                        </DataTrigger>
                                                        <DataTrigger Binding="{Binding $fieldName.Type, Converter={local:DataConverter}, ConverterParameter=FieldTypeText}"
                                                                     Value="Double">
                                                            <Setter Property="Height" Value="0"/>
                                                        </DataTrigger>
                                                        <DataTrigger Binding="{Binding $fieldName.Type, Converter={local:DataConverter}, ConverterParameter=FieldTypeText}"
                                                                     Value="DateOnly">
                                                            <Setter Property="Height" Value="0"/>
                                                        </DataTrigger>
                                                    </Style.Triggers>

                                                </Style>
                                            </RowDefinition.Style>
                                        </RowDefinition>
                                    </Grid.RowDefinitions>
                                    <TextBox Grid.Row="0" d:Text="{
                                        Binding $fieldName.Value, 
                                        Converter={local:DataConverter Parameter={l:ParameterizedResource FieldType}}, 
                                        Mode=TwoWay, 
                                        UpdateSourceTrigger=PropertyChanged
                                        }">
                                        <TextBox.Text>
                                            <Binding Path="$fieldName.Value" Mode="TwoWay" UpdateSourceTrigger="PropertyChanged" Converter="{local:DataConverter}">
                                                <Binding.ConverterParameter>
                                                    <l:BindingProxy Value="{Binding $fieldName.Type}"/>
                                                </Binding.ConverterParameter>
                                            </Binding>
                                        </TextBox.Text>
                                        <TextBox.Style>
                                            <Style TargetType="TextBox">
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding $fieldName.Type, Converter={local:DataConverter}, ConverterParameter=FieldTypeText}"
                                             Value="Int32">
                                                        <Setter Property="TextAlignment" Value="Right"/>
                                                    </DataTrigger>
                                                    <DataTrigger Binding="{Binding $fieldName.Type, Converter={local:DataConverter}, ConverterParameter=FieldTypeText}"
                                             Value="Double">
                                                        <Setter Property="TextAlignment" Value="Right"/>
                                                    </DataTrigger>
                                                    <DataTrigger Binding="{Binding $fieldName.Type, Converter={local:DataConverter}, ConverterParameter=FieldTypeText}"
                                             Value="Boolean">
                                                        <Setter Property="TextAlignment" Value="Center"/>
                                                    </DataTrigger>
                                                    <DataTrigger Binding="{Binding $fieldName.Type, Converter={local:DataConverter}, ConverterParameter=FieldTypeText}"
                                             Value="Activities">
                                                        <Setter Property="TextAlignment" Value="Center"/>
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </TextBox.Style>
                                    </TextBox>
                                    <ComboBox Grid.Row="1" SelectedItem="{Binding $fieldName.Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
                                        <ComboBox.Style>
                                            <Style TargetType="ComboBox">
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding $fieldName.Type, Converter={local:DataConverter}, ConverterParameter=FieldTypeText}"
                                                                     Value="Boolean">
                                                        <Setter Property="ItemsSource">
                                                            <Setter.Value>
                                                                <x:Array Type="sys:Boolean">
                                                                    <sys:Boolean>True</sys:Boolean>
                                                                    <sys:Boolean>False</sys:Boolean>
                                                                </x:Array>
                                                            </Setter.Value>
                                                        </Setter>
                                                    </DataTrigger>
                                                    <DataTrigger Binding="{Binding $fieldName.Type, Converter={local:DataConverter}, ConverterParameter=FieldTypeText}"
                                                                     Value="Activities">
                                                        <Setter Property="ItemsSource" Value="{Binding Converter={local:DataConverter}, ConverterParameter=Activities}"/>
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </ComboBox.Style>
                                    </ComboBox>
                                </Grid>
                            </DockPanel>
                        </DataTemplate>
                        <local:TableDataTemplateSelector x:Key="DataTemplate" x:Shared="False" FieldName="{l:ParameterizedResource $fieldName}"
                                                 IsEditable="{l:ParameterizedResource $isEditable}" EditValue="{l:ParameterizedResource EditField, Replaces=$template:StringTemplate}">
                            <local:TableDataTemplateSelector.ReadValue>
                                <DataTemplate>
                                    <TextBlock Text="{Binding $fieldName.Value}">
                                        <TextBlock.Style>
                                            <Style TargetType="TextBlock">
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding $fieldName.Type, Converter={local:DataConverter}, ConverterParameter=FieldTypeText}"
                                             Value="Int32">
                                                        <Setter Property="TextAlignment" Value="Right"/>
                                                    </DataTrigger>
                                                    <DataTrigger Binding="{Binding $fieldName.Type, Converter={local:DataConverter}, ConverterParameter=FieldTypeText}"
                                             Value="Double">
                                                        <Setter Property="TextAlignment" Value="Right"/>
                                                    </DataTrigger>
                                                    <DataTrigger Binding="{Binding $fieldName.Type, Converter={local:DataConverter}, ConverterParameter=FieldTypeText}"
                                             Value="Boolean">
                                                        <Setter Property="TextAlignment" Value="Center"/>
                                                    </DataTrigger>
                                                    <DataTrigger Binding="{Binding $fieldName.Type, Converter={local:DataConverter}, ConverterParameter=FieldTypeText}"
                                             Value="Activities">
                                                        <Setter Property="TextAlignment" Value="Center"/>
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </TextBlock.Style>
                                    </TextBlock>
                                </DataTemplate>
                            </local:TableDataTemplateSelector.ReadValue>
                        </local:TableDataTemplateSelector>
                        <DataTemplate x:Key="IsReadOnly">
                            <CheckBox IsChecked="{Binding IsReadOnly, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                        </DataTemplate>
                        <DataTemplate x:Key="Actions">
                            <StackPanel Orientation="Horizontal">
                                <Button Content="➖" Command="{Binding Value, Source={StaticResource RemoveCommand}}" ToolTip="Remove row">
                                    <Button.CommandParameter>
                                        <MultiBinding Converter="{local:DataConverter}" ConverterParameter="RemoveCommandCanExecute">
                                            <Binding Path="."/>
                                            <Binding Path="Value" Source="{StaticResource IsEditable}"/>
                                        </MultiBinding>
                                    </Button.CommandParameter>
                                </Button>
                            </StackPanel>
                        </DataTemplate>
                    </DataGrid.Resources>
                    <DataGrid.Columns>
                        <DataGridTemplateColumn Header="IsReadOnly" CellEditingTemplate="{StaticResource IsReadOnly}">
                            <DataGridTemplateColumn.CellTemplate>
                                <DataTemplate>
                                    <TextBlock Text="{Binding IsReadOnly}"/>
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
                        </DataGridTemplateColumn>
                        <DataGridTemplateColumn Header="Item1"
                                                CellTemplateSelector="{l:ParameterizedResource DataTemplate, Replaces=$fieldName:Item1|$isEditable:NotEditable}"
                                                CellEditingTemplateSelector="{l:ParameterizedResource DataTemplate, Replaces=$fieldName:Item1|$isEditable:IsEditable}"/>
                        <DataGridTemplateColumn Header="Item2" 
                                                CellTemplateSelector="{l:ParameterizedResource DataTemplate, Replaces=$fieldName:Item2|$isEditable:NotEditable}"
                                                CellEditingTemplateSelector="{l:ParameterizedResource DataTemplate, Replaces=$fieldName:Item2|$isEditable:IsEditable}"/>
                        <DataGridTemplateColumn Header="Item3" 
                                                CellTemplateSelector="{l:ParameterizedResource DataTemplate, Replaces=$fieldName:Item3|$isEditable:NotEditable}"
                                                CellEditingTemplateSelector="{l:ParameterizedResource DataTemplate, Replaces=$fieldName:Item3|$isEditable:IsEditable}"/>
                        <DataGridTemplateColumn Header="Item4" 
                                                CellTemplateSelector="{l:ParameterizedResource DataTemplate, Replaces=$fieldName:Item4|$isEditable:NotEditable}"
                                                CellEditingTemplateSelector="{l:ParameterizedResource DataTemplate, Replaces=$fieldName:Item4|$isEditable:IsEditable}"/>
                        <DataGridTemplateColumn Header="Item5" 
                                                CellTemplateSelector="{l:ParameterizedResource DataTemplate, Replaces=$fieldName:Item5|$isEditable:NotEditable}"
                                                CellEditingTemplateSelector="{l:ParameterizedResource DataTemplate, Replaces=$fieldName:Item5|$isEditable:IsEditable}"/>
                        <DataGridTemplateColumn Header="Actions" CellTemplate="{StaticResource Actions}"/>
                    </DataGrid.Columns>
                </DataGrid>
            </DockPanel>
        </TabItem>
    </TabControl>
</Window>
