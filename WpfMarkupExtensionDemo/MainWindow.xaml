            </ResourceDictionary.MergedDictionaries>
            <Style TargetType="CheckBox" x:Key="CheckBoxClickStyle">
                <EventSetter Event="Click" Handler="CheckBoxClick"/>
            </Style>
            <Style TargetType="Button" x:Key="ButtonMouseEventStyle">
                <EventSetter Event="MouseEnter" Handler="ButtonMouseEnter"/>
                <EventSetter Event="MouseLeave" Handler="ButtonMouseLeave"/>
                <EventSetter Event="PreviewMouseDown" Handler="ButtonMouseDown"/>
                <EventSetter Event="PreviewMouseUp" Handler="ButtonMouseUp"/>
            </Style>
            <Style TargetType="Button" x:Key="ButtonRepaintColStyle" x:Shared="False">
                <Style.Triggers>
                    <DataTrigger Binding="{Binding CurrentMouseEnter, Converter={local:DataConverter}, ConverterParameter=$coordinates|IsMouseEnter}" Value="True">
                        <Setter Property="Background">
                            <Setter.Value>
                                <MultiBinding Converter="{local:DataConverter}" ConverterParameter="Background">
                                    <Binding ElementName="Grid"/>
                                    <Binding Source="$coordinates"/>
                                </MultiBinding>
                            </Setter.Value>
                        </Setter>
                        <Setter Property="Foreground">
                            <Setter.Value>
                                <MultiBinding Converter="{local:DataConverter}" ConverterParameter="Foreground">
                                    <Binding ElementName="Grid"/>
                                    <Binding Source="$coordinates"/>
                                </MultiBinding>
                            </Setter.Value>
                        </Setter>
                    </DataTrigger>
                </Style.Triggers>
            </Style>
            <Style TargetType="Button" x:Key="ButtonRepaintRowStyle" x:Shared="False">
                <Style.Triggers>
                    <DataTrigger Binding="{Binding CurrentMouseEnter, Converter={local:DataConverter}, ConverterParameter=$coordinates|IsMouseEnter}" Value="True">
                        <Setter Property="BorderThickness">
                            <Setter.Value>
                                <MultiBinding Converter="{local:DataConverter}" ConverterParameter="BorderThickness">
                                    <Binding ElementName="Grid"/>
                                    <Binding Source="$coordinates"/>
                                </MultiBinding>
                            </Setter.Value>
                        </Setter>
                        <Setter Property="BorderBrush">
                            <Setter.Value>
                                <MultiBinding Converter="{local:DataConverter}" ConverterParameter="BorderBrush">
                                    <Binding ElementName="Grid"/>
                                    <Binding Source="$coordinates"/>
                                </MultiBinding>
                            </Setter.Value>
                        </Setter>
                    </DataTrigger>
                </Style.Triggers>
            </Style>
            <Style TargetType="Button" x:Key="ButtonAnimationStyle" x:Shared="False">
                <Style.Triggers>
                    <DataTrigger Binding="{Binding CurrentMouseDown, Converter={local:DataConverter}, ConverterParameter=$coordinates|IsMouseDown}" Value="True">
                        <DataTrigger.EnterActions>
                            <BeginStoryboard>
                                <Storyboard>
                                    <DoubleAnimation Storyboard.TargetProperty="Opacity" From="1" To="0" Duration="0:0:0.5" AutoReverse="True" RepeatBehavior="1x" />
                                </Storyboard>
                            </BeginStoryboard>
                        </DataTrigger.EnterActions>
                    </DataTrigger>
                </Style.Triggers>
            </Style>
            <Style TargetType="Button" x:Key="ButtonContentStyle" x:Shared="False">
                <Setter Property="Content" Value="{Binding RelativeSource={RelativeSource Self}, Converter={local:DataConverter}, ConverterParameter=Content}"/>
                <Setter Property="FontSize" Value="22.0"/>
            </Style>
            <l:BindingProxy x:Key="IsEditable" Value="{Binding IsDatasEditable}"/>
            <l:BindingProxy x:Key="NotEditable" Value="{Binding Source=False}"/>
            <l:BindingProxy x:Key="RemoveCommand" Value="{Binding RemoveCommand}"/>
        </ResourceDictionary>
